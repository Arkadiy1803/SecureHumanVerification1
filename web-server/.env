# Server Configuration
PORT=3000
NODE_ENV=development
HOST=0.0.0.0

# Security
API_SECRET=your-secret-key-change-this-12345
SESSION_SECRET=session-secret-change-this-67890
CORS_ORIGIN=http://localhost:3000

# Telegram Bot Integration
BOT_WEBHOOK_URL=http://localhost:8080/webhook
BOT_API_SECRET=bot-secret-key

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info
LOG_FILE=web-server/logs/server.log

# Data Storage
DATA_DIR=web-server/data
MAX_FILE_SIZE=10485760  # 10MB

# Features
ENABLE_GEOLOCATION=true
ENABLE_FINGERPRINTING=true
ENABLE_ANALYTICS=true
